package comA;

import java.util.ArrayList;

public class Demo2 {

	public static void main(String[] args) {
		ArrayList a1 = new ArrayList();
		a1.add(100);
		a1.add(200);
		a1.add(300);
		a1.add(400);
		a1.add(500);
		
		for(int i = 0; i < a1.size(); i++) {
			System.out.println(a1.get(i));
		}
	}

}
